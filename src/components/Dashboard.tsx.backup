'use client'

import { useEffect, useState } from 'react'
import { motion } from 'framer-motion'
import { Card, CardContent, CardDescription, CardHeader, CardTitle } from '@/components/ui/card'
import { Button } from '@/components/ui/button'
import { Badge } from '@/components/ui/badge'
import { Progress } from '@/components/ui/progress'
import { Download, Loader2, FileText, Zap, Flame, Target, Dumbbell, Apple, Droplets } from 'lucide-react'
import { toast } from '@/hooks/use-toast'

interface NutritionData {
  bmr: number
  tdee: number
  targetCalories: number
  proteinGrams: number
  carbsGrams: number
  fatsGrams: number
  proteinPercent: number
  carbsPercent: number
  fatsPercent: number
}

const StatCard = ({ icon: Icon, title, value, description, delay }: any) => (
  <motion.div
    initial={{ opacity: 0, y: 20, scale: 0.95 }}
    animate={{ opacity: 1, y: 0, scale: 1 }}
    transition={{ delay, duration: 0.5 }}
    whileHover={{ 
      scale: 1.05, 
      boxShadow: '0 20px 40px -12px rgba(0, 0, 0, 0.3)' 
    }}
  >
    <Card className="bg-gradient-to-br from-gray-900/40 to-gray-800/30 dark:from-gray-800/50 dark:to-gray-900/40 border-border/50 backdrop-blur-sm h-full cursor-pointer transition-all duration-300">
      <CardHeader>
        <motion.div 
          className="flex items-center gap-2 mb-2"
          whileHover={{ x: 5 }}
          transition={{ duration: 0.2 }}
        >
          <Icon className="w-5 h-5 text-foreground/70" />
          <CardDescription>{title}</CardDescription>
        </motion.div>
        <CardTitle className="text-3xl font-bold bg-gradient-to-r from-foreground to-foreground/70 bg-clip-text text-transparent">
          {Math.round(value)}
        </CardTitle>
      </CardHeader>
      <CardContent>
        <p className="text-sm text-muted-foreground">{description}</p>
      </CardContent>
    </Card>
  </motion.div>
)

export default function Dashboard() {
  const [nutrition, setNutrition] = useState<NutritionData | null>(null)
  const [loading, setLoading] = useState(true)
  const [downloading, setDownloading] = useState(false)

  useEffect(() => {
    fetchNutritionData()
  }, [])

  const fetchNutritionData = async () => {
    try {
      const response = await fetch('/api/nutrition')
      if (response.ok) {
        const data = await response.json()
        setNutrition(data)
      }
    } catch (error) {
      console.error('Failed to fetch nutrition data:', error)
    } finally {
      setLoading(false)
    }
  }

  const generatePlan = async () => {
    try {
      const response = await fetch('/api/generate-plan', { method: 'POST' })
      if (!response.ok) throw new Error('Failed to generate plan')
      
      // Only show success and fetch new data if response is ok
      const data = await response.json()
      if (data.success) {
        toast({ title: 'Success', description: 'Meal and workout plans generated!' })
        await fetchNutritionData()
      }
    } catch (error) {
      console.error('Error generating plan:', error)
      toast({ title: 'Error', description: 'Failed to generate plan. Please try again.', variant: 'destructive' })
    }
  }

  const downloadPDF = async () => {
    setDownloading(true)
    try {
      // Fetch all data needed for the PDF
      const [profileRes, nutritionRes, mealPlanRes, workoutPlanRes, progressRes] = await Promise.all([
        fetch('/api/profile'),
        fetch('/api/nutrition'),
        fetch('/api/meal-plan'),
        fetch('/api/workout-plan'),
        fetch('/api/progress'),
      ])

      const profile = await profileRes.json()
      const nutritionData = await nutritionRes.json()
      const mealPlan = await mealPlanRes.json()
      const workoutPlan = await workoutPlanRes.json()
      const progress = await progressRes.json()

      if (!profile || !nutritionData) {
        throw new Error('Missing required data for PDF generation')
      }

      // Create HTML content
      const htmlContent = generatePDFHTML(profile, nutritionData, mealPlan, workoutPlan, progress)

      // Create a blob and download
      const blob = new Blob([htmlContent], { type: 'text/html' })
      const url = URL.createObjectURL(blob)
      const a = document.createElement('a')
      a.href = url
      a.download = `mn-smart-nutrition-${new Date().toISOString().split('T')[0]}.html`
      document.body.appendChild(a)
      a.click()
      document.body.removeChild(a)
      URL.revokeObjectURL(url)

      toast({ 
        title: 'PDF Generated Successfully', 
        description: 'Your plan has been exported. Open in browser and print to PDF.' 
      })
    } catch (error) {
      console.error('PDF generation error:', error)
      toast({ 
        title: 'Error', 
        description: 'Failed to generate PDF. Please try again.', 
        variant: 'destructive' 
      })
    } finally {
      setDownloading(false)
    }
  }

  const generatePDFHTML = (profile: any, nutrition: any, mealPlan: any, workoutPlan: any, progress: any) => {
    return `
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MN Smart Nutrition - Personalized Plan</title>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&family=Space+Grotesk:wght@400;500;600;700&display=swap');
        
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        body {
            font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
            background: linear-gradient(135deg, #0a0a0a 0%, #1a1a1a 50%, #0d0d0d 100%);
            color: #e5e5e5;
            line-height: 1.6;
            min-height: 100vh;
        }
        
        .container {
            max-width: 900px;
            margin: 0 auto;
            padding: 60px 40px;
            background: rgba(20, 20, 20, 0.95);
            box-shadow: 0 25px 50px -12px rgba(0, 0, 0, 0.5);
        }
        
        .header {
            text-align: center;
            margin-bottom: 60px;
            padding-bottom: 40px;
            border-bottom: 2px solid linear-gradient(90deg, #333 0%, #555 100%);
            border-bottom: 2px solid #333;
        }
        
        .logo {
            font-family: 'Space Grotesk', sans-serif;
            font-size: 48px;
            font-weight: 700;
            background: linear-gradient(135deg, #ffffff 0%, #a0a0a0 100%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            margin-bottom: 15px;
        }
        
        .subtitle {
            font-size: 14px;
            color: #666;
            font-weight: 400;
            letter-spacing: 3px;
            text-transform: uppercase;
        }
        
        .date {
            margin-top: 20px;
            color: #555;
            font-size: 13px;
        }
        
        h1 {
            font-family: 'Space Grotesk', sans-serif;
            font-size: 36px;
            font-weight: 700;
            color: #ffffff;
            margin: 40px 0 25px 0;
            display: flex;
            align-items: center;
            gap: 15px;
        }
        
        h1::before {
            content: '';
            width: 4px;
            height: 36px;
            background: linear-gradient(180deg, #ffffff 0%, #666 100%);
            border-radius: 2px;
        }
        
        h2 {
            font-family: 'Space Grotesk', sans-serif;
            font-size: 24px;
            font-weight: 600;
            color: #ffffff;
            margin: 30px 0 20px 0;
        }
        
        .metrics-grid {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 25px;
            margin: 30px 0;
        }
        
        .metric-card {
            background: linear-gradient(145deg, #1a1a1a 0%, #252525 100%);
            border: 1px solid #333;
            border-radius: 16px;
            padding: 30px 25px;
            text-align: center;
            position: relative;
            overflow: hidden;
        }
        
        .metric-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 3px;
            background: linear-gradient(90deg, #555 0%, #333 100%);
        }
        
        .metric-value {
            font-family: 'Space Grotesk', sans-serif;
            font-size: 42px;
            font-weight: 700;
            color: #ffffff;
            margin-bottom: 8px;
        }
        
        .metric-label {
            font-size: 12px;
            color: #666;
            text-transform: uppercase;
            letter-spacing: 1px;
            font-weight: 500;
        }
        
        .metrics-divider {
            grid-column: 1 / -1;
            height: 1px;
            background: linear-gradient(90deg, transparent 0%, #333 50%, transparent 100%);
            margin: 20px 0;
        }
        
        .macros-grid {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 20px;
            margin: 30px 0;
        }
        
        .macro-card {
            background: linear-gradient(145deg, #1a1a1a 0%, #222 100%);
            border: 1px solid #333;
            border-radius: 12px;
            padding: 25px 20px;
            text-align: center;
        }
        
        .macro-name {
            font-family: 'Space Grotesk', sans-serif;
            font-size: 18px;
            font-weight: 600;
            color: #ffffff;
            margin-bottom: 12px;
        }
        
        .macro-value {
            font-family: 'Space Grotesk', sans-serif;
            font-size: 36px;
            font-weight: 700;
            color: #ffffff;
            margin-bottom: 8px;
        }
        
        .macro-unit {
            font-size: 18px;
            color: #888;
            font-weight: 400;
        }
        
        .macro-percent {
            font-size: 13px;
            color: #666;
            background: #222;
            padding: 4px 12px;
            border-radius: 20px;
            display: inline-block;
            margin-top: 8px;
        }
        
        .section-box {
            background: linear-gradient(145deg, #151515 0%, #1a1a1a 100%);
            border: 1px solid #333;
            border-radius: 16px;
            padding: 35px;
            margin: 35px 0;
        }
        
        .meal-card, .workout-card {
            background: #1a1a1a;
            border: 1px solid #333;
            border-radius: 10px;
            padding: 20px;
            margin: 15px 0;
        }
        
        .meal-title, .workout-title {
            font-family: 'Space Grotesk', sans-serif;
            font-size: 20px;
            font-weight: 600;
            color: #ffffff;
            margin-bottom: 15px;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }
        
        .meal-calories {
            font-size: 14px;
            color: #666;
            background: #222;
            padding: 5px 12px;
            border-radius: 15px;
        }
        
        .food-item, .exercise-item {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 12px 0;
            border-bottom: 1px solid #2a2a2a;
        }
        
        .food-item:last-child, .exercise-item:last-child {
            border-bottom: none;
        }
        
        .food-name {
            font-size: 14px;
            color: #ccc;
        }
        
        .food-macros {
            font-size: 12px;
            color: #666;
            text-align: right;
        }
        
        .meal-total {
            margin-top: 15px;
            padding-top: 15px;
            border-top: 1px solid #333;
            font-size: 13px;
            color: #888;
        }
        
        .progress-table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 20px;
        }
        
        .progress-table th {
            font-family: 'Space Grotesk', sans-serif;
            font-size: 13px;
            font-weight: 600;
            color: #888;
            text-align: left;
            padding: 15px;
            border-bottom: 2px solid #333;
            text-transform: uppercase;
            letter-spacing: 0.5px;
        }
        
        .progress-table td {
            font-size: 14px;
            color: #ccc;
            padding: 15px;
            border-bottom: 1px solid #2a2a2a;
        }
        
        .progress-table tr:last-child td {
            border-bottom: none;
        }
        
        .info-box {
            background: linear-gradient(135deg, #1a1a1a 0%, #151515 100%);
            border-left: 3px solid #444;
            padding: 20px;
            margin: 25px 0;
            border-radius: 0 8px 8px 0;
        }
        
        .info-box p {
            font-size: 13px;
            color: #888;
            line-height: 1.7;
        }
        
        .info-box strong {
            color: #aaa;
        }
        
        .footer {
            margin-top: 60px;
            padding-top: 40px;
            border-top: 1px solid #333;
            text-align: center;
        }
        
        .footer-text {
            font-size: 13px;
            color: #666;
            margin-bottom: 8px;
        }
        
        .footer-credit {
            font-family: 'Space Grotesk', sans-serif;
            font-size: 14px;
            color: #888;
            font-weight: 500;
        }
        
        .badge {
            display: inline-block;
            padding: 4px 12px;
            background: linear-gradient(135deg, #333 0%, #222 100%);
            border-radius: 20px;
            font-size: 11px;
            color: #888;
            text-transform: uppercase;
            letter-spacing: 1px;
            margin-left: 10px;
        }
        
        @media print {
            body { -webkit-print-color-adjust: exact; print-color-adjust: exact; }
        }
    </style>
</head>
<body>
    <div class="container">
        <!-- Header -->
        <div class="header">
            <div class="logo">MN Smart Nutrition</div>
            <div class="subtitle">Precision Nutrition & Fitness Tracking</div>
            <div class="date">Generated on ${new Date().toLocaleDateString('en-US', { 
                weekday: 'long', 
                year: 'numeric', 
                month: 'long', 
                day: 'numeric' 
            })}</div>
        </div>

        ${nutrition ? `
        <!-- Nutrition Overview -->
        <h1>Calorie Targets <span class="badge">Daily</span></h1>
        
        <div class="metrics-grid">
            <div class="metric-card">
                <div class="metric-value">${Math.round(nutrition.bmr)}</div>
                <div class="metric-label">BMR (kcal)</div>
            </div>
            <div class="metric-card">
                <div class="metric-value">${Math.round(nutrition.tdee)}</div>
                <div class="metric-label">TDEE (kcal)</div>
            </div>
            <div class="metric-card">
                <div class="metric-value">${Math.round(nutrition.targetCalories)}</div>
                <div class="metric-label">Target (kcal)</div>
            </div>
        </div>

        <!-- Macros -->
        <h1>Macronutrient Distribution</h1>
        
        <div class="macros-grid">
            <div class="macro-card">
                <div class="macro-name">Protein</div>
                <div class="macro-value">${Math.round(nutrition.proteinGrams)}<span class="macro-unit">g</span></div>
                <div class="macro-percent">${Math.round(nutrition.proteinPercent)}%</div>
            </div>
            <div class="macro-card">
                <div class="macro-name">Carbohydrates</div>
                <div class="macro-value">${Math.round(nutrition.carbsGrams)}<span class="macro-unit">g</span></div>
                <div class="macro-percent">${Math.round(nutrition.carbsPercent)}%</div>
            </div>
            <div class="macro-card">
                <div class="macro-name">Fats</div>
                <div class="macro-value">${Math.round(nutrition.fatsGrams)}<span class="macro-unit">g</span></div>
                <div class="macro-percent">${Math.round(nutrition.fatsPercent)}%</div>
            </div>
        </div>

        <div class="info-box">
            <p><strong>Calculation Method:</strong> Mifflin-St Jeor equation with activity and goal adjustments. All values are estimates and should be adjusted based on your progress and response.</p>
        </div>
        ` : ''}

        ${mealPlan ? `
        <!-- Meal Plan -->
        <h1>Meal Plan</h1>
        
        <div class="section-box">
            <div class="metrics-grid">
                <div class="metric-card">
                    <div class="metric-value">${Math.round(mealPlan.dailyTotal.calories)}</div>
                    <div class="metric-label">Daily Calories</div>
                </div>
                <div class="metric-card">
                    <div class="metric-value">${Math.round(mealPlan.dailyTotal.protein)}g</div>
                    <div class="metric-label">Protein</div>
                </div>
                <div class="metric-card">
                    <div class="metric-value">${Math.round(mealPlan.dailyTotal.carbs)}g</div>
                    <div class="metric-label">Carbs</div>
                </div>
            </div>

            ${mealPlan.meals.map((meal: any) => `
                <div class="meal-card">
                    <div class="meal-title">
                        ${meal.name}
                        <span class="meal-calories">${Math.round(meal.totalCalories)} kcal</span>
                    </div>
                    ${meal.foods.map((food: any) => `
                        <div class="food-item">
                            <span class="food-name">${food.portion} ${food.name}</span>
                            <span class="food-macros">${Math.round(food.calories)} kcal • P:${Math.round(food.protein)}g C:${Math.round(food.carbs)}g F:${Math.round(food.fats)}g</span>
                        </div>
                    `).join('')}
                    <div class="meal-total">
                        Meal Total: ${Math.round(meal.totalCalories)} kcal • P: ${Math.round(meal.totalProtein)}g • C: ${Math.round(meal.totalCarbs)}g • F: ${Math.round(meal.totalFats)}g
                    </div>
                </div>
            `).join('')}
        </div>
        ` : ''}

        ${workoutPlan ? `
        <!-- Workout Plan -->
        <h1>Workout Plan</h1>
        
        <div class="section-box">
            <h2>${workoutPlan.weeklySplit}</h2>
            
            ${JSON.parse(workoutPlan.workoutDays).map((day: any) => `
                <div class="workout-card">
                    <div class="workout-title">${day.day} <span class="meal-calories">${day.focus}</span></div>
                    ${day.exercises.map((ex: any) => `
                        <div class="exercise-item">
                            <span style="color: #ccc; font-size: 14px;">${ex.name}</span>
                            <span style="color: #888; font-size: 13px;">${ex.sets} × ${ex.reps} • Rest: ${ex.rest}</span>
                        </div>
                    `).join('')}
                </div>
            `).join('')}
        </div>
        ` : ''}

        ${progress && progress.length > 0 ? `
        <!-- Progress History -->
        <h1>Progress History</h1>
        
        <div class="section-box">
            <table class="progress-table">
                <thead>
                    <tr>
                        <th>Date</th>
                        <th>Weight</th>
                        <th>Body Fat</th>
                        <th>Notes</th>
                    </tr>
                </thead>
                <tbody>
                    ${progress.slice(0, 10).map((entry: any) => `
                        <tr>
                            <td>${new Date(entry.date).toLocaleDateString()}</td>
                            <td style="font-weight: 500;">${entry.weight} kg</td>
                            <td>${entry.bodyFatPercent ? entry.bodyFatPercent + '%' : '-'}</td>
                            <td>${entry.notes || '-'}</td>
                        </tr>
                    `).join('')}
                </tbody>
            </table>
        </div>
        ` : ''}

        <!-- Footer -->
        <div class="footer">
            <p class="footer-text">MN Smart Nutrition - Precision Nutrition & Fitness Tracking</p>
            <p class="footer-text" style="margin-top: 8px; font-size: 12px;">Disclaimer: This information is for educational purposes only and does not constitute medical advice.</p>
            <p class="footer-credit">Created by Mohammed Nasser - Software Development Engineer</p>
        </div>
    </div>
</body>
</html>
    `
  }

  if (loading) {
    return (
      <div className="flex items-center justify-center min-h-[400px]">
        <Loader2 className="w-8 h-8 animate-spin" />
      </div>
    )
  }

  if (!nutrition) {
    return (
      <Card className="bg-card/50 backdrop-blur-sm border-border/50">
        <CardHeader>
          <CardTitle>No Profile Found</CardTitle>
          <CardDescription>Please complete your profile first</CardDescription>
        </CardHeader>
      </Card>
    )
  }

  return (
    <div className="max-w-6xl mx-auto space-y-6">
      <div className="flex items-center justify-between">
        <div>
          <h2 className="text-3xl font-bold">Your Nutrition Dashboard</h2>
          <p className="text-muted-foreground">Personalized calorie and macro targets based on scientific calculations</p>
        </div>
        <div className="flex gap-2">
          <Button 
            onClick={generatePlan} 
            variant="outline"
            className="hover:bg-white/10 transition-all duration-300 hover:scale-105"
          >
            <FileText className="w-4 h-4 mr-2" />
            Generate Plan
          </Button>
          <Button 
            onClick={downloadPDF}
            disabled={downloading}
            className="bg-gradient-to-r from-gray-800 to-gray-900 hover:from-gray-700 hover:to-gray-800 transition-all duration-300 hover:scale-105 shadow-lg"
          >
            {downloading ? (
              <>
                <Loader2 className="w-4 h-4 mr-2 animate-spin" />
                Generating...
              </>
            ) : (
              <>
                <Download className="w-4 h-4 mr-2" />
                Download PDF
              </>
            )}
          </Button>
        </div>
      </div>

      {/* Calorie Targets */}
      <div className="grid grid-cols-1 md:grid-cols-3 gap-4">
        <StatCard 
          icon={Zap}
          title="Basal Metabolic Rate"
          value={nutrition.bmr}
          description="Calories/day at rest"
          delay={0.1}
        />
        <StatCard 
          icon={Flame}
          title="Total Daily Energy"
          value={nutrition.tdee}
          description="Calories/day with activity"
          delay={0.2}
        />
        <StatCard 
          icon={Target}
          title="Target Calories"
          value={nutrition.targetCalories}
          description="Adjusted for your goal"
          delay={0.3}
        />
      </div>

      {/* Macros */}
      <motion.div
        initial={{ opacity: 0, y: 20 }}
        animate={{ opacity: 1, y: 0 }}
        transition={{ delay: 0.4, duration: 0.5 }}
        whileHover={{ scale: 1.01 }}
      >
        <Card className="bg-card/50 backdrop-blur-sm border-border/50">
          <CardHeader>
            <CardTitle className="flex items-center gap-2">
              <Dumbbell className="w-5 h-5 text-foreground/70" />
              Daily Macro Targets
            </CardTitle>
            <CardDescription>Precise macro distribution for optimal results</CardDescription>
          </CardHeader>
          <CardContent className="space-y-6">
            <div className="grid grid-cols-1 md:grid-cols-3 gap-6">
              {/* Protein */}
              <motion.div 
                className="space-y-2"
                whileHover={{ x: 5 }}
                transition={{ duration: 0.2 }}
              >
                <div className="flex items-center justify-between">
                  <div className="flex items-center gap-2">
                    <Droplets className="w-4 h-4 text-foreground/70" />
                    <span className="font-medium">Protein</span>
                  </div>
                  <Badge variant="secondary" className="transition-all duration-300 hover:scale-110">{Math.round(nutrition.proteinPercent)}%</Badge>
                </div>
                <p className="text-2xl font-bold bg-gradient-to-r from-foreground to-foreground/70 bg-clip-text text-transparent">{Math.round(nutrition.proteinGrams)}g</p>
                <Progress value={nutrition.proteinPercent} className="h-2" />
                <p className="text-xs text-muted-foreground">
                  Essential for muscle repair and growth
                </p>
              </motion.div>

              {/* Carbs */}
              <motion.div 
                className="space-y-2"
                whileHover={{ x: 5 }}
                transition={{ duration: 0.2 }}
              >
                <div className="flex items-center justify-between">
                  <div className="flex items-center gap-2">
                    <Apple className="w-4 h-4 text-foreground/70" />
                    <span className="font-medium">Carbohydrates</span>
                  </div>
                  <Badge variant="secondary" className="transition-all duration-300 hover:scale-110">{Math.round(nutrition.carbsPercent)}%</Badge>
                </div>
                <p className="text-2xl font-bold bg-gradient-to-r from-foreground to-foreground/70 bg-clip-text text-transparent">{Math.round(nutrition.carbsGrams)}g</p>
                <Progress value={nutrition.carbsPercent} className="h-2" />
                <p className="text-xs text-muted-foreground">
                  Primary energy source for training
                </p>
              </motion.div>

              {/* Fats */}
              <motion.div 
                className="space-y-2"
                whileHover={{ x: 5 }}
                transition={{ duration: 0.2 }}
              >
                <div className="flex items-center justify-between">
                  <span className="font-medium">Fats</span>
                  <Badge variant="secondary" className="transition-all duration-300 hover:scale-110">{Math.round(nutrition.fatsPercent)}%</Badge>
                </div>
                <p className="text-2xl font-bold bg-gradient-to-r from-foreground to-foreground/70 bg-clip-text text-transparent">{Math.round(nutrition.fatsGrams)}g</p>
                <Progress value={nutrition.fatsPercent} className="h-2" />
                <p className="text-xs text-muted-foreground">
                  Hormonal health and nutrient absorption
                </p>
              </motion.div>
            </div>

            {/* Calculation Method */}
            <motion.div 
              className="bg-gradient-to-br from-gray-900/20 to-transparent dark:from-gray-800/30 dark:to-transparent rounded-lg p-4 border border-border/50"
              whileHover={{ scale: 1.02, boxShadow: '0 10px 30px -10px rgba(0, 0, 0, 0.2)' }}
              transition={{ duration: 0.3 }}
            >
              <p className="text-sm">
                <span className="font-semibold">Method:</span> Calculated using Mifflin-St Jeor equation 
                with activity and goal adjustments. All values are estimates and should be adjusted based 
                on your progress and response.
              </p>
            </motion.div>
          </CardContent>
        </Card>
      </motion.div>
    </div>
  )
}
